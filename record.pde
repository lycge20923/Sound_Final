void saveArray(int[] array, String filename) {
  StringBuilder sb = new StringBuilder();
  for (int i = 0; i < array.length; i++) {
    sb.append(array[i]);
    if (i < array.length - 1) {
      sb.append(", ");
    }
  }

  // Save the string to the text file
  saveStrings(filename, new String[]{sb.toString()});
  
  println("IntArray saved to " + filename);
}

void saveArray(long[] array, String filename) {
  StringBuilder sb = new StringBuilder();
  for (int i = 0; i < array.length; i++) {
    sb.append(array[i]);
    if (i < array.length - 1) {
      sb.append(", ");
    }
  }

  // Save the string to the text file
  saveStrings(filename, new String[]{sb.toString()});
  
  println("LongArray saved to " + filename);
}

int[] loadIntArray(String filename) {
  // 读取字符串数组
  String[] stringArray = loadStrings(filename);

  // 将字符串数组转换为 int 数组
  int[] intArray = new int[stringArray.length];
  for (int i = 0; i < stringArray.length; i++) {
    intArray[i] = int(stringArray[i]);
  }

  return intArray;
}

long[] loadLongArray(String filename) {
  // 读取字符串数组
  String[] stringArray = loadStrings(filename);

  // 将字符串数组转换为 int 数组
  long[] intArray = new long[stringArray.length];
  for (int i = 0; i < stringArray.length; i++) {
    intArray[i] = int(stringArray[i]);
  }

  return intArray;
}

void replay(){
    if(press_idx < length_of_record && millis() - replay_start_time >= keyPressTimes[press_idx]){
        // println("press : " + press_idx, keyPressTimes[press_idx], keyPressCodes[press_idx]);
        performKeyPress(keyPressCodes[press_idx]);
        press_idx++;
    }
    if(release_idx < length_of_record && millis() - replay_start_time >= keyReleaseTimes[release_idx]){
        // println("release : " + release_idx, keyReleaseTimes[release_idx], keyReleaseCodes[release_idx]);
        performKeyRelease(keyReleaseCodes[release_idx]);
        release_idx++;
    }
    if(release_idx >= length_of_record) {
        println("replay finished");
        isReplay = false;
        leftKeyPressed = false;
        rightKeyPressed = false;
        upKeyPressed = false;
        downKeyPressed = false;
    }
}

void performKeyPress(int code) {
  if (code == LEFT) {
    leftKeyPressed = true;
  }else if (code == RIGHT) {
    rightKeyPressed = true;
  }else if (code == UP) {
    upKeyPressed = true;
  }else if (code == DOWN) {
    downKeyPressed = true;
  }
}
void performKeyRelease(int code) {
  if (code == LEFT) {
    leftKeyPressed = false;
  }else if (code == RIGHT) {
    rightKeyPressed = false;
  }else if (code == UP) {
    upKeyPressed = false;
  }else if (code == DOWN) {
    downKeyPressed = false;
  }
}
